<form method="post" action="{{ path('group_zone_delete', {'idGroupZone': group_zone.idGroupZone}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ group_zone.idGroupZone) }}">
    <button class="btn">Delete</button>
</form>
